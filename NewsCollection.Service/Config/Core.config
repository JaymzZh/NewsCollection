<?xml version="1.0" encoding="utf-8"?>
<!--核心设置-->
<Setting>
  <!--全局调试-->
  <Debug>true</Debug>
  <!--日志等级，只输出大于等于该级别的日志-->
  <LogLevel>Debug</LogLevel>
  <!--文本日志目录-->
  <LogPath>Log</LogPath>
  <!--临时目录-->
  <TempPath>XTemp</TempPath>
  <!--网络调试-->
  <NetDebug>false</NetDebug>
  <!--多线程调试-->
  <ThreadDebug>false</ThreadDebug>
  <!--网页压缩文件-->
  <WebCompressFiles>.aspx,.axd,.js,.css</WebCompressFiles>
</Setting>